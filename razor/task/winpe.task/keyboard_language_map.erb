def get_keyboard_language_pair(key, val)
  locales = JSON.parse( get_keyboard_language_map )
  if (values=(res = locales.select { |k| !(locales[k][key] =~ /#{val}/i).nil? }).values).length >= 1
    [ res.keys ]
  else
    raise "Found '#{values.length}' candidates found for key '#{key}', val '#{val}' [%s]" % res.collect{ |k,v| v }.join(",")
  end
end

